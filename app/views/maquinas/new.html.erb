<h1>Nova Máquina</h1>

<div class="row">
  <div class="col-md-9">
    <%= render 'form', maquina: @maquina %>
  </div>
  <div class="col-md-3">
    <h3>Máquinas já cadastradas</h3>
    <% Maquina.reorder("descricao ASC").each do |m| %>
      <li><%= m.descricao %></li>
    <% end %>
  </div>
</div>
